{
  "name": "functions",
  "description": "Sapper SSR on Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "npm run build && firebase serve",
    "serve_node": "npm run build && node __sapper__/build",
    "serve_functions": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start_functions": "npm run shell",
    "deploy": "npm run build && firebase deploy",
    "logs": "firebase functions:log",
    "build_ts": "tsc src/firebase/*.ts --outDir lib/firebase --tsBuildInfoFile lib/firebase/build.json --incremental --esModuleInterop",
    "build_sapper": "sapper build",
    "build": "make firebase && sapper build",
    "dev": "make firebase && sapper dev",
    "export": "sapper export",
    "serve_export": "npx serve __sapper__/export",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/firebase/functions.js",
  "dependencies": {
    "compression": "^1.7.1",
    "express": "4.17.1",
    "firebase": "^8.6.3",
    "firebase-admin": "^9.3.0",
    "firebase-auth": "^0.1.2",
    "firebase-functions": "^3.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@firebase/app": "^0.6.11",
    "@firebase/storage": "^0.5.2",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/highlight.js": "^9.12.4",
    "@types/node": "^14.14.6",
    "cookie-parser": "^1.4.5",
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.2.0",
    "highlight.js": "^10.3.2",
    "lodash": "^4.17.21",
    "marked": ">=2.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.10",
    "sirv": "^1.0.10",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-loading-spinners": "^0.1.2",
    "svelte-preprocess": "^4.6.6",
    "tslib": "^2.1.0",
    "typescript": "^4.0.3"
  },
  "private": true
}
