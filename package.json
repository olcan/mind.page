{
  "name": "functions",
  "description": "Sapper SSR on Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "npm run build && firebase serve",
    "serve_node": "npm run build && node __sapper__/build",
    "serve_functions": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start_functions": "npm run shell",
    "deploy": "npm run build && firebase deploy",
    "logs": "firebase functions:log",
    "build_ts": "tsc src/firebase/*.ts --outDir lib/firebase --tsBuildInfoFile lib/firebase/build.json --incremental --esModuleInterop",
    "build_sapper": "sapper build && (pkill -f \"server.js$|sapper dev$\"||true)",
    "build": "make firebase && sapper build && (pkill -f \"server.js$|sapper dev$\"||true)",
    "dev": "make firebase && ./run_sapper_dev",
    "export": "sapper export",
    "serve_export": "npx serve __sapper__/export",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "main": "lib/firebase/functions.js",
  "dependencies": {
    "@types/mime": "^3.0.4",
    "compression": "^1.7.4",
    "express": "^4.21.1",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.6.0",
    "firebase-auth": "^0.1.2",
    "firebase-functions": "^6.0.1",
    "http-proxy-middleware": "^3.0.3",
    "npm": "^10.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/runtime": "^7.25.7",
    "@firebase/app": "^0.10.13",
    "@firebase/storage": "^0.13.2",
    "@jupyterlab/services": "^7.2.5",
    "@octokit/rest": "^21.0.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@rollup/plugin-url": "^8.0.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/compression": "^1.7.5",
    "@types/node": "^22.7.7",
    "chokidar": "^4.0.1",
    "cookie-parser": "^1.4.7",
    "eslint": "^9.13.0",
    "eslint-plugin-promise": "^7.1.0",
    "firebase-functions-test": "^3.3.0",
    "ip": "^2.0.1",
    "js-sha1": "^0.7.0",
    "lodash": "^4.17.21",
    "marked": "^14.1.3",
    "marked-extended-tables": "^1.0.10",
    "marked-highlight": "^2.2.0",
    "murmurhash3js-revisited": "^3.0.0",
    "rollup": "^4.24.0",
    "rollup-plugin-svelte": "^7.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.3",
    "sirv": "^3.0.0",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.5",
    "svelte-loading-spinners": "^0.3.6",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.6.3",
    "ua-parser-js": "^1.0.39",
    "utf8": "^3.0.0"
  },
  "private": true
}
