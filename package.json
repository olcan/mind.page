{
  "name": "functions",
  "description": "Sapper SSR on Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "npm run build && firebase serve",
    "serve_node": "npm run build && node __sapper__/build",
    "serve_functions": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start_functions": "npm run shell",
    "deploy": "npm run build && firebase deploy",
    "logs": "firebase functions:log",
    "build_ts": "tsc src/firebase/*.ts --outDir lib/firebase --tsBuildInfoFile lib/firebase/build.json --incremental --esModuleInterop",
    "build_sapper": "sapper build && (pkill -f \"server.js$|sapper dev$\"||true)",
    "build": "make firebase && sapper build && (pkill -f \"server.js$|sapper dev$\"||true)",
    "dev": "make firebase && ./run_sapper_dev",
    "export": "sapper export",
    "serve_export": "npx serve __sapper__/export",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/firebase/functions.js",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "4.18.2",
    "firebase": "^9.12.1",
    "firebase-admin": "11.1.0",
    "firebase-auth": "^0.1.2",
    "firebase-functions": "^3.24.1",
    "http-proxy-middleware": "^2.0.6",
    "npm": "^8.19.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/runtime": "^7.19.4",
    "@firebase/app": "^0.8.2",
    "@firebase/storage": "^0.9.12",
    "@jupyterlab/services": "^6.4.8",
    "@octokit/rest": "^19.0.5",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@rollup/plugin-url": "^8.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/compression": "^1.7.2",
    "@types/node": "^18.11.0",
    "chokidar": "^3.5.3",
    "cookie-parser": "^1.4.6",
    "eslint": "^8.25.0",
    "eslint-plugin-promise": "^6.1.0",
    "firebase-functions-test": "^2.4.0",
    "js-sha1": "^0.6.0",
    "lodash": "^4.17.21",
    "marked": "^4.1.1",
    "murmurhash3js-revisited": "^3.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.3",
    "sirv": "^2.0.2",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-loading-spinners": "^0.3.2",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.8.4",
    "ua-parser-js": "^1.0.32",
    "utf8": "^3.0.0"
  },
  "private": true
}
